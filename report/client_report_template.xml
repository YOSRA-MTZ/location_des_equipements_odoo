<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="etudiant_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Rapport client</h2>
                        <p><strong>Nom:</strong> <t t-esc="o.name"/></p>
                         <p><strong>CIN:</strong> <t t-esc="o.cin"/></p>
                        <p><strong>Date de naissance:</strong> <t t-esc="o.date_naissance"/></p>
                        <p><strong>Email:</strong> <t t-esc="o.email"/></p>
                        <p><strong>Téléphone:</strong> <t t-esc="o.phone"/></p>
                        <p><strong>Adress:</strong> <t t-esc="o.adress"/></p>
                        <p><strong>Sexe:</strong> <t t-esc="o.sexe"/></p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
